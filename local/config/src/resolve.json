{
  "extensions": [".tsx", ".ts", ".jsx", ".js", ".json"],
  "styleExtensions": [".css", ".scss", ".sass"],
  "directories": ["node_modules"],
  "paths": {
    "jest": {
      "moduleNameMapper": {
        "^~/(.*)": "<rootDir>/src/$1"
      }
    },
    "rollupTypes": {
      "~/*": ["<typesDir>/*"]
    }
  }
}
