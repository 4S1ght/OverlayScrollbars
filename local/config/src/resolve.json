{
  "extensions": [".json", ".js", ".jsx", ".cjs", ".mjs", ".ts", ".tsx"],
  "styleExtensions": [".scss", ".sass", ".css"],
  "directories": ["node_modules"],
  "paths": {
    "jest": {
      "moduleNameMapper": {
        "^~/(.*)": "<rootDir>/src/$1"
      }
    },
    "rollupAlias": {
      "^~/(.*)": "<rootDir>/src/$1"
    },
    "rollupTypes": {
      "~/*": ["<typesDir>/*"]
    }
  }
}
